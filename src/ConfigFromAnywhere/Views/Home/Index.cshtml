@{
    ViewData["Title"] = "Home Page";
    var enabled = ViewBag.ButtonEnabled;
}


<div class="row">
    <div class="col-md-12">
        @if (enabled)
        {
            <div><a href="#" class="btn btn-lg btn-success">Press Me.</a></div>
        }
        else
        {
            <div>The button is disabled.</div>

        }
    </div>

</div>
